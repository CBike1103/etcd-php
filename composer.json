{
    "name": "youzusg/etcd-php",
    "description": "Etcd client library for PHP with basic auth function",
    "homepage": "https://github.com/youzusg/etcd-php",
    "keywords": [
        "php",
        "etcd",
        "api",
        "distributed",
        "configuration"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Chen Fang",
            "email": "crossfire1103@gmail.com",
            "role": "Development"
        }
    ],
    "require": {
